api/Aluno/listar',{\n            id: id\n          }).then(function(response) {\n            moduloAluno.total = response.data.length;\n            var AlunoDados = response.data[0];\n            this.nome= AlunoDados.nome;\n            this.curso=AlunoDados.curso;\n            this.turma=AlunoDados.turma;\n            this.ano=AlunoDados.ano;\n            this.email=AlunoDados.email;\n            this.senha=AlunoDados.senha;\n            });\n    },\n\t\tlistarAluno(){\n      axios.post(config.dominio+'api/Aluno/listar',{\n      }).then(function(response) {\n        this.total = response.data.length;\n        this.dados = response.data;\n      });\n    },\n    verAluno(id){\n          axios.post(config.dominio+'api/Aluno/listar',{\n            id: id\n          }).then(function(response) {\n            moduloAluno.total = response.data.length;\n            moduloAluno.dados = response.data[0];\n          });\n\n\n        },\n\n    excluirAluno(id) {\n\n      alertify.confirm(\"Deseja excluir o cadastro?\",\n        function(){\n          axios.post(config.dominio+'api/Aluno/excluir', {\n            id: id\n          }).then(function(response) {\n\n\n            if(response.status==200){\n\n            alertify.success('Sucesso ao deletar cadastro!');\n            moduloAluno.listar();\n            }else{\n\n              alertify.error('Erro no servidor ao deletar cadastro!');\n\n            }\n\n          });\n        },\n        function(){\n          alertify.warning('Exclus√£o de cadastro cancelada');\n        });\n\n    }\n  },\n  created() {\n    this.listarAluno();\n  }\n\n\n}\n\nVue.createApp(moduloAluno).mount('#app').component('listAluno', {template:listAluno});\n"]}]}