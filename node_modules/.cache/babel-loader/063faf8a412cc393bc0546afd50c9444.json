        email:AlunoDados.email;\n            senha:AlunoDados.senha;\n            });\n    },\n\t\tlistarAluno(){\n\t      axios.post(config.dominio+'api/Aluno/listar', {\n\t      }).then(function(response) {\n\t        moduloAluno.total = response.data.length;\n\t        moduloAluno.dados = response.data;\n\t      });\n\n\n\t  },\n    verAluno(id){\n          axios.post(config.dominio+'api/Aluno/listar',{\n            id: id\n          }).then(function(response) {\n            moduloAluno.total = response.data.length;\n            moduloAluno.dados = response.data[0];\n          });\n\n\n        },\n\n    excluirAluno(id) {\n\n      alertify.confirm(\"Deseja excluir o cadastro?\",\n        function(){\n          axios.post(config.dominio+'api/Aluno/excluir', {\n            id: id\n          }).then(function(response) {\n\n\n            if(response.status==200){\n\n            alertify.success('Sucesso ao deletar cadastro!');\n            moduloAluno.listar();\n            }else{\n\n              alertify.error('Erro no servidor ao deletar cadastro!');\n\n            }\n\n          });\n        },\n        function(){\n          alertify.warning('Exclus√£o de cadastro cancelada');\n        });\n\n    }\n  },\n  created() {\n    this.listarAluno();\n  }\n\n\n}\nnew Vue(moduloAluno);\n/**\nmodule.exports = {\n  pages: {\n    formAluno: {\n      entry: 'Aluno.js',\n      template: './src/components/formaluno.html',\n      filename: 'aluno.html',\n      title: 'aluno',\n      chunks: ['chunk-vendors', 'chunk-common', 'index']\n    },\n    listarAluno: {\n      entry: 'Aluno.js',\n      template: './src/components/listaaluno.vue',\n      filename: 'aluno.html',\n      title: 'aluno',\n      chunks: ['chunk-vendors', 'chunk-common', 'index']\n    }\n  }\n}**/\n"]}]}