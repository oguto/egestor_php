GH;AACR,SAAKN,WAAL;AACD;AAhHY,CAAf","sourcesContent":["import axios from 'axios'\n\nconst config={dominio:'http://localhost/freelas/egestor/'}\n\n\nexport default {\n  name: 'my-component',\n  data() {\n    return {\n        nome:'',\n        curso:'',\n        turma:'',\n        ano:'',\n        email:'',\n        senha:'',\n        linha:'',\n        lista:'',\n        total: 0\n    };\n  },\n  methods: {\n    incluirAluno: function() {},\n    salvarAluno: function(){\n          var url ='';\n\n          if(this.id==null){\n\n            url = config.dominio+'api/Aluno/incluir';\n\n          }else {\n\n            url = config.dominio+'api/Aluno/editar';\n\n          }\n          axios.post(url, {\n            nome:this.nome,\n            curso:this.curso,\n            turma:this.turma,\n            ano:this.ano,\n            email:this.email,\n            senha:this.senha,\n            }).then(function(response) {\n\n            this.listar();\n\n            if(response.status==200){\n                this.$alertify.success('Sucesso ao alterar o cadastro!');\n\n            }else{\n\n              this.$alertify.error('Houve erro no servidor ao alterar cadastro!');\n\n            }\n\n          });\n\n    },\n    editarAluno: function(id){\n          axios.post(config.dominio+'api/Aluno/listar',{\n            id: id\n          }).then(function(response) {\n            this.total = response.data.length;\n            var AlunoDados = response.data[0];\n            this.nome= AlunoDados.nome;\n            this.curso=AlunoDados.curso;\n            this.turma=AlunoDados.turma;\n            this.ano=AlunoDados.ano;\n            this.email=AlunoDados.email;\n            this.senha=AlunoDados.senha;\n            });\n    },\n    listarAluno: function(){\n      axios.post(config.dominio+'api/Aluno/listar',{\n      }).then(function(response) {\n        this.total = response.data.length;\n        this.lista = response.data;\n      });\n    },\n    verAluno: function(id){\n          axios.post(config.dominio+'api/Aluno/listar',{\n            id: id\n          }).then(function(response) {\n            this.total = response.data.length;\n            this.dados = response.data[0];\n          });\n\n\n        },\n    excluirAluno: function(id) {\n\n      this.$alertify.confirm(\"Deseja excluir o cadastro?\",\n        function(){\n          axios.post(config.dominio+'api/Aluno/excluir', {\n            id: id\n          }).then(function(response) {\n\n\n            if(response.status==200){\n\n            this.$alertify.success('Sucesso ao deletar cadastro!');\n            this.listar();\n            }else{\n\n              this.$alertify.error('Erro no servidor ao deletar cadastro!');\n\n            }\n\n          });\n        },\n        function(){\n          this.$alertify.warning('Exclusão de cadastro cancelada');\n        });\n\n    }\n  },\n  created() {\n    this.listarAluno();\n  }\n\n\n}\n"]}]}